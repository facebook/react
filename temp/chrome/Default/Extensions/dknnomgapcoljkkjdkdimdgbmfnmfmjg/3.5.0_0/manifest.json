{
   "applications": {
      "gecko": {
         "id": "intern_new_tab@fb",
         "strict_min_version": "68.0"
      }
   },
   "background": {
      "persistent": true,
      "scripts": [ "background.js", "bunnylolautocomplete.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png"
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "description": "Customizes the new tab experience for FB employees and provides autocomplete search for bunnylol (type `b` on the search bar).",
   "homepage_url": "https://fb.workplace.com/groups/internalnewtab/",
   "icons": {
      "16": "bunny_16.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "43",
   "name": "Internal New Tab",
   "omnibox": {
      "keyword": "b"
   },
   "permissions": [ "storage", "*://*.internalfb.com/", "*://fburl.com/", "bookmarks", "contextMenus", "clipboardWrite", "notifications", "tabs", "webRequest", "webRequestBlocking" ],
   "update_url": "https://www.internalfb.com/intern/browser_extensions/chrome/update.xml/",
   "version": "3.5.0"
}
