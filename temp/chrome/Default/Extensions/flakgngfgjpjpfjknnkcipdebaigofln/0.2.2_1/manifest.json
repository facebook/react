{
   "background": {
      "scripts": [ "wss_background.js" ]
   },
   "content_scripts": [ {
      "js": [ "wss_content.js" ],
      "matches": [ "https://*.facebook.com/*", "https://*.internalfb.com/*" ]
   } ],
   "description": "bunnylol screenshare",
   "externally_connectable": {
      "matches": [ "https://*.facebook.com/*", "https://*.internalfb.com/*" ]
   },
   "icons": {
      "128": "screenshare-ext-128.png",
      "16": "screenshare-ext-16.png",
      "48": "screenshare-ext-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "70",
   "name": "Wireless Screen Sharing",
   "permissions": [ "desktopCapture", "notifications", "https://*.facebook.com/", "https://*.internalfb.com/" ],
   "update_url": "https://www.internalfb.com/intern/browser_extensions/chrome/update.xml",
   "version": "0.2.2"
}
